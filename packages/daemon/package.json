{
  "name": "@dfinity/http-proxy-daemon",
  "version": "0.0.3-alpha",
  "description": "Daemon process to enable trustless access to the Internet Computer.",
  "main": "built/main.js",
  "types": "built/main.d.ts",
  "author": "Kepler Vital <kepler.vital@dfinity.org>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/dfinity/http-proxy/tree/main/packages/daemon#readme",
  "repository": {
    "url": "https://github.com/dfinity/http-proxy.git",
    "type": "git",
    "directory": "packages/daemon"
  },
  "bin": "./built/start.js",
  "scripts": {
    "start": "node ./built/start.js",
    "dev": "nodemon",
    "build": "tsc && tsc-alias",
    "lint": "eslint --ext ts,js src",
    "lint:fix": "eslint --ext ts,js --fix src",
    "pkg": "pkg ."
  },
  "pkg": {
    "targets": [
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "compress": "GZip",
    "outputPath": "bin"
  },
  "keywords": [
    "Internet Computer",
    "IC",
    "Proxy",
    "HTTP",
    "HTTP Proxy",
    "IC Response Verification"
  ],
  "engines": {
    "node": ">=18",
    "npm": "please-use-yarn",
    "pnpm": "please-use-yarn",
    "yarn": "~3"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/node-forge": "^1.3.1",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.1",
    "prettier": "^2.8.4",
    "tsc-alias": "^1.8.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@dfinity/http-proxy-core": "0.0.3-alpha",
    "http-proxy": "^1.18.1",
    "node-cache": "^5.1.2",
    "node-forge": "^1.3.1",
    "pako": "^2.1.0",
    "pino": "^8.11.0"
  }
}
